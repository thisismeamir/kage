{
  "name": "Kage for Kid A",
  "base_path": "/home/kid-a/sandbox/kage",
  "paths": ["/home/kid-a/kage"],
  "version": "1.0.0",
  "logging": {
    "level": "info",
    "file": "/logs/kage.log"
  },
  "server": {
    "port": 8080,
    "host": "localhost",
    "api": {
      "base_url": "/api",
      "version": "v1"
    },
    "database": []
  },
  "client": {
    "Web": {
      "port": 3000,
      "host": "localhost",
      "network_accessible": false,
      "path": "./web",
      "build": "npm build",
      "run": "npm run"
    }
  },
  "default_execution_policy": {
    "cpu_usage_threshold": 80,
    "memory_usage_threshold": 100,
    "disk_usage_threshold": 100,
    "network_usage_threshold": 100000
  }
}
